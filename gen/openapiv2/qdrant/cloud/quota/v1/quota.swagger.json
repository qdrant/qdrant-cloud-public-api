    "/api/quota/v1/accounts/{accountId}/quotas": {
      "get": {
        "summary": "Lists all quotas for the account identified by the given account ID.\nRequired permissions:\n- read:account",
        "operationId": "QuotaService_ListAccountQuotas",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/v1ListAccountQuotasResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/googlerpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "accountId",
            "description": "The identifier of the account (in GUID format) to list quotas for.\nThis is a required field.",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "QuotaService"
        ]
      }
    },
    "/api/quota/v1/users/me/quotas": {
